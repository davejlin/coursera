class Controls {

  function void draw() {
    do Output.moveCursor(1, 0);
    do Output.printString("Controls:");
    do Output.moveCursor(2, 0);
    do Output.printString("Arrow - move block");
    do Output.moveCursor(3, 0);
    do Output.printString("Space - rotate");
    do Output.moveCursor(4, 0);
    do Output.printString("Q - quit");
    return;
  }

}